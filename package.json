{
  "name": "flowdash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run migrate:up && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "migrate:create": "dotenv -e .env.local -- node-pg-migrate --config migrate-config.js create",
    "migrate:up": "dotenv -e .env.local -- node-pg-migrate --config migrate-config.js up",
    "migrate:down": "dotenv -e .env.local -- node-pg-migrate --config migrate-config.js down"
  },
