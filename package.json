    "@neondatabase/serverless": "^1.0.1",
    "@sentry/nextjs": "^10.2.0",
    "@types/js-cookie": "^3.0.6",
    "@types/pino": "^7.0.4",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^11.2.12",
    "geist": "^1.4.2",
    "i18next": "^25.3.2",
